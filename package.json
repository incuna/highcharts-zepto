{
  "name": "highcharts-zepto",
  "description": "This is an adapter for using Zepto.js with Highcharts rather than jQuery.",
  "version": "1.3.3",
  "author": "Highsoft Solutions AS <support@highcharts.com>",
  "homepage": "http://www.highcharts.com/",
  "keywords": ["charts", "vml", "svg"],
  "repository": {
    "type": "git",
    "url": "https://github.com/highslide-software/highcharts.com.git"
  },
  "licenses": [{
    "type": "CC BY-NC 3.0",
    "url": "http://creativecommons.org/licenses/by-nc/3.0/"
  },{
    "type": "Commercial",
    "url": "http://shop.highsoft.com/highstock.html"
  }],
  "categories": ["Visualization"],
  "jam": {
    "main": "highcharts.src.js",
    "include": ["highcharts-zepto.src.js", "highcharts.src.js"],
    "shim": {
      "deps": ["zepto", "highcharts-zepto/highcharts-zepto.src"],
      "exports": "Highcharts"
    },
    "dependencies": {
      "zepto": "1.0.0-rc1"

    }
  }
}
